  },
  "homepage": "https://github.com/s-a/iron-node#readme",
  "dependencies": {
    "commander": "^2.8.1",
    "deep-extend": "^0.4.0",
    "electron-prebuilt": "^0.37.5",
    "electron-recompile": "^1.0.16",
    "markdown": "^0.5.0",
    "nmp": "^1.0.3",
    "package.js": "^1.1.3",
    "pretty-error": "^2.0.0",
    "syntax-error": "^1.1.5"
  },
